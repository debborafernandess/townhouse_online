<h1><%= @townhouse_area.class.name %> <%= controller.action_name %></h1>

<%= form_for @townhouse_area do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="form-group">
  <%= f.label :area_type %>
  <%= f.select(:area_type,
               TownhouseArea.area_types.collect { |key, value| [key.humanize, key] },
               {}, class: 'form-control') %>
  </div>

  <div class="form-group">
    <%= f.submit 'Send', class: 'btn btn-default' %>
  </div>
<% end %>
